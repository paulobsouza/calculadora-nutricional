# Configuração do Servidor
PORT=3000

# MongoDB - Use sua string de conexão do MongoDB Atlas ou servidor próprio
MONGODB_URI=mongodb://localhost:27017/calculadora-nutricional

# JWT - IMPORTANTE: Gere uma chave segura para produção
# Use: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=GERE_UMA_CHAVE_SEGURA_AQUI
JWT_EXPIRES_IN=7d

# Ambiente
NODE_ENV=development
